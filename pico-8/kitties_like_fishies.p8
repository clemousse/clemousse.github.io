--load the hs
function loadhs()
 local _slot=0
 
 if dget(0)==1 then
  _slot+=1
  for i=1,5 do
   hs[i]=dget(_slot)
   hs1[i]=dget(_slot+1)
   hs2[i]=dget(_slot+2)
   hs3[i]=dget(_slot+3)
   _slot+=4
  end
  sorths()
 else
 --no file found or empty
 reseths()
 end
end

--save the data
function savehs()
 local _slot
 dset(0,1)
 --load the data
  _slot=1
  for i=1,5 do
  dset(_slot,hs[i])
  dset(_slot+1,hs1[i])
  dset(_slot+2,hs2[i])
  dset(_slot+3,hs3[i])
   _slot+=4
  end
end

