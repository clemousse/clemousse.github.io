-- main functions

function _init()
--cartdata for hs
	--cartdata("klc")
--time
	start_time=time()
--music
	sfx(3)
--scene
	scene='menu'
--end_hs
	end_hs='end'
--score
	fish=0	
	game_over=false
--sprites
	hearts={}
	create_heart()
	cats={}
	create_cat()
	dogs={}
	create_dog()
	fishies={}
	create_fish()
--explosions
	explosions={}
--highscore
	hs={}
	hs1={}
	hs2={}
	hs3={}
	reseths()
	--loadhs()
	addhs()
	hschars={"a","b","c","d","e",
	"f","g","h","i","j","k","l","m",
	"n","o","p","q","r","s","t","u",
	"v","w","x","y","z"}
	loghs=true
	--typing in initials
	nitials={1,1,1}
	nit_sel=1
	nit_conf=true
end

function _update()
	if scene=='menu' then
		update_menu()
	elseif scene=='game' then
		update_game()
	end
end

function _draw()
	if end_hs=='end' then
		if scene=='menu' then
			draw_menu()
		elseif scene=='game' then
			draw_game()	
		end
	else draw_hs()
	end
end

