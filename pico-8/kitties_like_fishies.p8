--interface game

function draw_fishies()
	camera()
	palt(0,false)
	palt(11,true)
	spr(38,1,1)
	palt()
	print_contour('X'..fish,13,2)
end

function print_contour(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end

